{
    "name": "react-form-shield",
    "version": "0.1.0",
    "description": "A comprehensive anti-spam solution for React forms",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "server"
    ],
    "sideEffects": false,
    "scripts": {
        "build": "tsc && rollup -c",
        "test": "jest --forceExit --runInBand",
        "lint": "eslint src --ext .ts,.tsx",
        "format": "prettier --write \"src/**/*.{ts,tsx}\"",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "docs": "node scripts/generate-docs.js",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "react",
        "form",
        "anti-spam",
        "security",
        "honeypot",
        "challenge",
        "bot-protection"
    ],
    "author": "Extension Team",
    "license": "GPL-3.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/extensionteamhq/react-form-shield.git"
    },
    "bugs": {
        "url": "https://github.com/extensionteamhq/react-form-shield/issues"
    },
    "homepage": "https://github.com/extensionteamhq/react-form-shield#readme",
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "devDependencies": {
        "@babel/core": "^7.23.0",
        "@babel/preset-env": "^7.23.0",
        "@babel/preset-react": "^7.22.15",
        "@babel/preset-typescript": "^7.23.0",
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-typescript": "^11.1.5",
        "@storybook/addon-actions": "^7.6.10",
        "@storybook/addon-essentials": "^7.6.10",
        "@storybook/addon-links": "^7.6.10",
        "@storybook/react": "^7.6.10",
        "@testing-library/jest-dom": "^6.1.5",
        "@testing-library/react": "^14.3.1",
        "@testing-library/react-hooks": "^8.0.1",
        "@types/jest": "^29.5.5",
        "@types/react": "^18.2.28",
        "@types/react-dom": "^18.2.13",
        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@typescript-eslint/parser": "^6.8.0",
        "eslint": "^8.51.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "prettier": "^3.0.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.54.2",
        "rollup": "^4.1.4",
        "rollup-plugin-dts": "^6.1.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-terser": "^7.0.2",
        "ts-jest": "^29.1.1",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.0.0"
    }
}